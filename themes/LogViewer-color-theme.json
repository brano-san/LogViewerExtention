{
  "$schema": "vscode://schemas/color-theme",
  "name": "LogViewer Theme",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "constant.other.logviewer.level.info",
      "settings": {
        "foreground": "#61c554",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.logviewer.level.warn",
      "settings": {
        "foreground": "#ffd166",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.logviewer.level.error",
      "settings": {
        "foreground": "#ef476f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.logviewer.level.debug",
      "settings": {
        "foreground": "#118ab2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.other.logviewer.level.fatal",
      "settings": {
        "foreground": "#ff6b6b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic.logviewer.timestamp",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "scope": "entity.name.section.logviewer.module",
      "settings": {
        "foreground": "#a0aec0"
      }
    },
    {
      "scope": "string.logviewer.title",
      "settings": {
        "foreground": "#e1e1e1"
      }
    }
  ]
}