{
  "scopeName": "source.logviewer",
  "patterns": [
    {
      "name": "markup.italic.logviewer.timestamp",
      "match": "^\\[[0-2]\\d:[0-5]\\d:[0-5]\\d:\\d{3}\\]"
    },
    {
      "name": "entity.name.section.logviewer.module",
      "match": "\\[\\s[^\\]]+\\s\\]"
    },
    {
      "name": "constant.other.logviewer.level.info",
      "match": "\\[I\\]"
    },
    {
      "name": "constant.other.logviewer.level.warn",
      "match": "\\[W\\]"
    },
    {
      "name": "constant.other.logviewer.level.error",
      "match": "\\[E\\]"
    },
    {
      "name": "constant.other.logviewer.level.debug",
      "match": "\\[D\\]"
    },
    {
      "name": "constant.other.logviewer.level.fatal",
      "match": "\\[F\\]"
    },
    {
      "name": "string.logviewer.title",
      "match": "(?<=\\]\\s)[^\\[][^\\n]*"
    }
  ]
}